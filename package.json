{
  "name": "mccal-api",
  "version": "1.0.0",
  "description": "McCal Media API - Portfolio manifests service",
  "main": "src/api/server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "api:start": "node src/api/server.js",
